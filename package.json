{
    "name": "supr-text",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "next dev",
        "build:vercel": "pnpm run prisma:generate && next build",
        "build": "pnpm run prisma:push:dev && next build",
        "start": "next start",
        "build:analyze": "cross-env ANALYZE=true next build",
        "lint": "eslint **/*.{js,ts,tsx}",
        "lint:fix": "eslint **/*.{js,ts,tsx} --fix",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "prisma:push": "prisma db push",
        "prisma:generate": "prisma generate",
        "prisma:generate:dev": "env-cmd -f .env.development.local prisma generate",
        "prisma:push:dev": "env-cmd -f .env.development.local prisma db push",
        "prisma:generate:prod": "env-cmd -f .env.production.local prisma generate",
        "prisma:push:prod": "env-cmd -f .env.production.local prisma db push",
        "keygen:dev": "env-cmd -f .env.development.local ts-node keygen.ts abcd",
        "keygen:prod": "env-cmd -f .env.production.local ts-node keygen.ts abcd",
        "commit": "cz",
        "prepare": "husky install"
    },
    "dependencies": {
        "@nanostores/react": "^0.3.0",
        "@next/bundle-analyzer": "^12.3.4",
        "@prisma/client": "^4.8.1",
        "@vercel/og": "^0.0.27",
        "axios": "^0.27.2",
        "bcrypt": "^5.1.0",
        "cryptr": "^6.1.0",
        "formik": "^2.2.9",
        "framer-motion": "^7.10.3",
        "immer": "^9.0.17",
        "markdown-loader": "^8.0.0",
        "nanostores": "^0.6.0",
        "next": "^13.1.2",
        "next-api-middleware": "^2.0.1",
        "next-compose-plugins": "^2.2.1",
        "next-seo": "^5.15.0",
        "nextjs-cors": "^2.1.2",
        "phosphor-icons": "^1.4.2",
        "phosphor-react": "^1.4.1",
        "pino": "^8.8.0",
        "prism-react-renderer": "^1.3.5",
        "randomstring": "^1.2.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.0",
        "react-markdown": "^8.0.4",
        "react-query": "^3.39.2",
        "react-simple-code-editor": "^0.11.3",
        "react-use": "^17.4.0",
        "remark-gfm": "^3.0.1",
        "sass": "^1.57.1",
        "winston": "^3.8.2",
        "zustand": "^4.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-links": "^6.5.15",
        "@storybook/react": "^6.5.15",
        "@types/bcrypt": "^5.0.0",
        "@types/cryptr": "^4.0.1",
        "@types/node": "^18.11.18",
        "@types/randomstring": "^1.1.8",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "commitizen": "^4.2.6",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "env-cmd": "^10.1.0",
        "eslint": "^8.32.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.27.4",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.3",
        "prisma": "^4.8.1",
        "react-is": "^18.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "pnpm lint:fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
